Skipcheck;


  [ {'Shares - Current Number of Shares for James Fisher PLC Basic','Shares - Diluted Number of Shares'}, 'Monthly'] =
    IF ( LONG ( !Period ) = 4 & ELLEV ( 'Period', !Period ) > 0,
      IF ( SUBST ( !Period, 1, 4 ) @=  SUBST ( DB('Admin','Current Month','Input'), 1, 4 ),
        DB ( 'Reporting Input', !Actuality, STR ( MIN ( NUMBR (!Period | '12' ), Numbr ( DB('Admin','Current Month','Input') ) ), 6, 0 ), !'Reporting Input Account', 'Full Year' ),
        DB ( 'Reporting Input', !Actuality, !Period | '12', !'Reporting Input Account', 'Full Year' )
       ),
    CONTINUE );

  [ {'Shares - Current Number of Shares for James Fisher PLC Basic','Shares - Diluted Number of Shares'}] =
    IF ( ELLEV ( 'Period', !Period ) > 0,
        DB ( 'Reporting Input', !Actuality, ELCOMP ( 'Period', !Period, ELCOMPN ( 'Period', !Period ) ), !'Reporting Input Account', !m Reporting Input ),
    CONTINUE );


